{
  "extends": [
    "@nuxtjs/eslint-config-typescript"
  ],
  "rules": {
    // TS already checks this
    "no-use-before-define": "off",
    "@typescript-eslint/no-unused-vars": [
      "off"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ]
  }
}
